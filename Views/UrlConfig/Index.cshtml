@model IEnumerable<TrueDynamicWeb.Models.Url_Config_tabl>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Endpoint_name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.url)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.url_param)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.interval_number)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Report_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Status)
        </th>
        <th>
